{% set pathbrowse = currentPath %}
{% set multiselect = 1 %}
{% set multiselectFilesOnly = false %}

{% extends "flm::dialog-window.twig" %}
{% block heading %}
        <legend>{{ theUILang.fDiagCopySel }} </legend>
{% endblock %}

{% block scripts %}
<script>
(function (global) {
    var dialogs = flm.ui.getDialogs();

    dialogs.onStart(function () {
        dialogs.disableStartButton();
        flm.manager.doCopy(dialogs.getTargetPath(), dialogs.getCheckedList()).then(
                function () { dialogs.hide();},
                function (reason) {
                        console.log(reason);
                        log(reason);
                        dialogs.hide();
                }
        );
    });


})
(window);
</script>
{% endblock %}

{% set startButtonValue = theUILang.fDiagCopyBut %}
